{
  "name": "public_html",
  "version": "1.0.0",
  "main": "index.js",
  "description": "LGBT National Help Center Volunteer Console",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --config tests/jest.config.js",
    "test:watch": "jest --config tests/jest.config.js --watch",
    "test:coverage": "jest --config tests/jest.config.js --coverage",
    "test:debug": "DEBUG_TESTS=true jest --config tests/jest.config.js --verbose",
    "test:unit": "jest --config tests/jest.config.js --testPathPattern=unit",
    "test:integration": "jest --config tests/jest.config.js --testPathPattern=integration",
    "test:php": "cd tests && ../vendor/bin/phpunit",
    "test:all": "npm run test && npm run test:php",
    "build": "parcel build index.html",
    "start": "parcel index.html --port 1234"
  },
  "dependencies": {
    "@twilio/voice-sdk": "^2.15.0",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "https": "^1.0.0",
    "socket.io": "^4.7.5",
    "twilio-client": "^1.15.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
